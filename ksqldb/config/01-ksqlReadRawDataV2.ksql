CREATE STREAM wallbox__v2__id (
        MQTT_TOPIC VARCHAR KEY, 
        status VARCHAR,
        event VARCHAR,
        amount BIGINT,
        action VARCHAR,
        max BIGINT,
        response STRUCT <
            amount BIGINT,
            max BIGINT
        >
  ) WITH (
        KAFKA_TOPIC='wallbox__source', 
        VALUE_FORMAT='JSON'
  );
