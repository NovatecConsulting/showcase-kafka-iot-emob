CREATE STREAM wallbox_charge__v2__filter
    WITH (
        KAFKA_TOPIC='wallbox_charge__v2__sink',
        VALUE_FORMAT='JSON'
  ) AS SELECT
     CLIENT_ID_KEY,
     CLIENT_ID,
     PROTOCOL_TRANSLATION(STATUS,EVENT,ACTION,AMOUNT,MAX,RESPONSE_AMOUNT,RESPONSE_MAX,RESPONSE_STATUS,ERROR) AS CHARGE,
     TIMESTAMP
FROM wallbox__v2__filter
WHERE
     MQTT_TOPIC='charge';
